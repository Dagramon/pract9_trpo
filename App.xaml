<Application x:Class="pract7_trpo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:pract7_trpo"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/Colors/DefaultColors.xaml"/>
                <ResourceDictionary Source="/UI/GUI.xaml"/>
                <ResourceDictionary Source="/UI/Panels.xaml"/>
                <ResourceDictionary Source="/UI/Buttons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
